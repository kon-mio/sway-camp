<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxy.swaycamp.mapper.ArticleSortMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zxy.swaycamp.domain.entity.ArticleSort">
        <id column="id" property="id" />
        <result column="sort_name" property="sortName" />
        <result column="sort_description" property="sortDescription" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, sort_name, sort_description, is_deleted
    </sql>

</mapper>
